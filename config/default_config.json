{
    "name": "BOTOMETER_DNN",
    "seed":8,
    "run_val": 1,
    "dataset": {
        "root_dir" : "BR-Project",
        "BLOC": "data/features-bot-shuffled.csv",
        "BOTOMETER": "data/features-botometer_June27_Shuffled.csv"
    },
    "lr_scheduler": {
        "step_size": 50,
        "gamma": 0.1
    },
    "optimizer": {
        "lr": 5e-4,
        "mlp_weight_decay":0.0,
        "mnf_weight_decay":0.0,
        "KL_scale": 0.0001
    },
    "num_epochs":100,
    "dataloader": {
        "split":[0.7,0.15,0.15],
        "train": {
            "batch_size": 1024,
            "num_samples": 1000
    	},
        "val": {
            "batch_size": 75
        },
        "test": {
            "batch_size_BLOC": 75,
            "batch_size_BOTOMETER": 35
        }
    },
    "output": {
	"dir": "Trained_Models"
    },
    "Inference": {
        "MNF_model_BOTOMETER": "Trained_Models/BOTOMETER_BNN___Jun-27-2024/BOTOMETER_BNN_epoch15_val_loss_3.733104.pth",
        "DNN_model_BOTOMETER": "Trained_Models/BOTOMETER_DNN___Jun-27-2024/BOTOMETER_DNN_epoch11_val_loss_0.252369.pth",
        "MNF_model_BLOC": "Trained_Models/BLOC_BNN___Jun-27-2024/BLOC_BNN_epoch62_val_loss_0.948132.pth",
        "DNN_model_BLOC": "Trained_Models/BLOC_DNN___Jun-27-2024/BLOC_DNN_epoch46_val_loss_0.314640.pth",
        "out_dir_BLOC": "Aleatoric_Epistemic_BLOC",
        "out_dir_BOTOMETER": "Aleatoric_Epistemic_BOTOMETER",
        "out_file": "Inference.csv"
    }
}

